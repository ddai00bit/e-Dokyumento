      <!-- Header-->
      <header id="header" class="header">

          <div class="header-menu">

              <div class="col-sm-12">
                  <a id="menuToggle" class="menutoggle pull-left"><i class="fa fa fa-tasks"></i></a>
                  <div class="header-left">

                    <!-- notification-->
                      <div id="notifyme" class="dropdown for-notification float-left">
                          <button class="btn btn-secondary dropdown-toggle" type="button" id="notification" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                              <i class="fa fa-bell"></i>
                              <span id=notiNr class="count bg-danger"><%= files.length %></span>
                          </button>
                          <div class="dropdown-menu " aria-labelledby="notification">
                             <li> <p class="red dropdown-item"> You have  <%= files.length %> files pending</p></li>
                              <% for (var i=0; i < files.length; i++){ %>
                                  <li>
                                  <a class="dropdown-item media bg-flat-color-3" href="/incoming/<%= files[i] %>">
                                    <i class="fa fa-check"></i>
                                    <p><%= files[i] %></p>
                                  </a>
                                </li>
                                <% } %>

                          </div>
                      </div>
                      <form name="formroute" id="formroute">
                      <!-- form send to branch-->
                      <div class="col col-sm-3 float-left"><input value= "<%= disp %>" type="text" id="newfile" name="newfile" class="input-sm form-control-sm form-control"></div>
                      <div class=" float-left"><input value= "<%= disp %>" type="hidden" id="fileroute"  name="fileroute"></div>

                      <!-- if AI completes-->
                      <div id="actBr" class="float-left" style="display: none;">
                        <span class="badge badge-info">Action Branch</span>
                      </div>

                      <button id= "butScan" class="col col-sm-3 btn btn-primary float-left btn-sm" type="button" disabled>
                        <span class="spinner-border spinner-border-sm " role="status" aria-hidden="true"></span>
                      <span clas="col-sm-0 " id="txtscan" >&nbsp;Scanning document using OCR...</span>
                      </button>

                      <div class="col col-sm-2 float-left" id='selDiv' style="display: none;">

                        <select id="selBr" data-placeholder="Select Branch" multiple class="standardSelect" tabindex="5" name="selBr">
                              <option value="N6A" id="N6A">N6A</option>
                              <option value="N6B" id="N6B">N6B</option>
                              <option value="N6C" id="N6C">N6C</option>
                              <option value="N6D" id="N6D">N6D</option>
                              <option value="N6E" id="N6E">N6E</option>
                              <option value="N6F" id="N6F">N6F</option>
                              <option>All Branches</option>
                          <optgroup label="===========">
                              <option>Ex-O</option>
                              <option>DN6</option>
                              <option>N6</option>
                          </optgroup>
                        </select>
                      </div>
                      <div class="float-left" id='butDiv' style="display: none;"><button type="submit" class="btn btn-success btn-sm"><i class="fa fa-magic"></i>&nbsp; Route File</button></div>
                  </form>

              <div class="header-right">

